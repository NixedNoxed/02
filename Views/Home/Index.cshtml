@model List<Book>

<h2>Books</h2>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Author</th>
            <th>Price</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var book in Model)
        {
            if(book != null)
            {
                <tr>
                    <td>@book.Name</td>
                    <td>@book.Author</td>
                    <td>@book.Price</td>
                </tr>
            }
        }
    </tbody>
</table>
